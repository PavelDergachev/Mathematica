          RowBox[{"Plus", ",", "0.", ",", 
           RowBox[{"Sign", "[", 
            RowBox[{"Chop", "[", 
             RowBox[{
              RowBox[{"ListCorrelate", "[", 
               RowBox[{
                RowBox[{"{", 
                 RowBox[{"1", ",", 
                  RowBox[{"-", "1"}]}], "}"}], ",", "#"}], "]"}], ",", 
              "tol"}], "]"}], "   ", "]"}]}], "    ", "]"}]}]}], "&"}], ",", 
      "data"}], "]"}]}], ";"}], "\n"}], "\[IndentingNewLine]", 
