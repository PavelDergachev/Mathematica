 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"UnwrapPhase", "[", 
     RowBox[{
      RowBox[{"data_", "?", "VectorQ"}], ",", 
      RowBox[{"tol_:", "Pi"}], ",", 
      RowBox[{"inc_:", "2", " ", "Pi"}]}], "]"}], ":=", 
    RowBox[{"FixedPoint", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"#", "+", 
